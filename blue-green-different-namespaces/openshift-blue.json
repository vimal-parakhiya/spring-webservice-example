{
  "apiVersion": "v1",
  "kind": "List",
  "items": [
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "greeting-service"
      },
      "spec": {
        "template": {
          "metadata": {
            "labels": {
              "name": "greeting-service"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "greeting-service",
                "image": "172.30.1.1:5000/oc-greeting-blue/greeting-service",
                "ports": [
                  {
                    "containerPort": 8100,
                    "protocol": "TCP"
                  }
                ]
              }
            ]
          }
        },
        "replicas": 1,
        "selector": {
          "name": "greeting-service"
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "strategy": {
          "type": "Rolling"
        }
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Service",
      "metadata": {
        "name": "greeting-service"
      },
      "spec": {
        "ports": [
          {
            "name": "http",
            "protocol": "TCP",
            "port": 8100,
            "targetPort": 8100
          }
        ],
        "selector": {
          "name": "greeting-service",
          "deploymentconfig": "greeting-service"
        }
      }
    }
  ]
}